    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "codecov": "^2.1.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "flow-bin": "^0.48.0",
    "flow-typed": "^2.1.2",
    "ghpages": "0.0.10",
    "husky": "^0.13.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.1",
    "phantomjs-prebuilt": "^2.1.14",
    "power-assert": "^1.4.2",
    "prettier": "^1.3.1",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-dev-server": "^2.4.5"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/9renpoto/js#readme",
  "keywords": [
    "flow-type"
  ],
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/js.git"
  },
  "scripts": {
    "appveyor": "npm-run-all -p appveyor:*",
    "appveyor:mocha": "nyc mocha -- tmp/test",
    "appveyor:karma": "karma start --browsers=IE,Chrome,Firefox --files-pattern=tmp/test/*.test.js",
    "build": "npm-run-all -p build:*",
    "build:browserify": "mkdir dist && browserify src/foo.js -t babelify -o dist/browserify.bundle.js",
    "build:webpack": "webpack",
    "clean": "rimraf coverage dist",
    "coverage:karma": "codecov -f ./coverage/Firefox*/lcov.info",
    "coverage:mocha": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "deploy": "ghpages --path dist",
    "lint": "npm-run-all -p lint:*",
    "lint:flow": "flow",
    "lint:js": "eslint . --cache",
    "preappveyor": "babel src -d tmp/src && babel test -d tmp/test",
    "prebuild": "npm run clean",
    "predeploy": "npm run build",
    "prepush": "npm test",
    "test": "npm-run-all -p test:*",
    "test:karma": "karma start",
    "test:build": "npm run build",
    "test:lint": "npm run lint",
    "test:mocha": "nyc mocha --require babel-register -- test"
  }
}
